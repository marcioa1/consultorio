<!DOCTYPE html>
<html>
<head>
  <title>Consultorio</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag 'jquery' %>
  <%= csrf_meta_tag %>
</head>
<body>
  <%= render :partial=>'/layouts/menu' %>
  <% if flash[:notice] %>
    <p class="notice"><%= flash[:notice] %></p>
  <% end %>
  <% if flash[:error] %>
    <p class="error"><%= flash[:error] %></p>
  <% end %>
<%= yield %>

</body>
</html>
